<%= form_for @service_request, :url => { :action => :make_returned } do |f| %>

  <%= render Const::STR_URL_FORM_HEADER, :f => f,
             :heading => "#{Const::STR_LABEL_SERVICE_REQUEST} ##{f.object.id}" %>

  <%= render Const::STR_URL_FIELDS, :f => f,
                                    :mode => Const::STR_VALUE_CALLBACK %>

  <%= render Const::STR_URL_FORM_SUBMIT, :f => f,
             :caption => Const::STR_ACTION_SAVE_CHANGES %>

<% end %>
