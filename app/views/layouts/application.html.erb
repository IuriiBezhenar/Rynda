<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag %>
    <%= render Const::STR_URL_LAYOUTS_STYLESHEETS %>
    <%#= javascript_include_tag :defaults %>
    <%# no need after use jquery: = javascript_include_tag :defaults %>
    <%= javascript_include_tag "jquery", "jquery-ui", "rails" %>
  </head>
  <body>
    <div class ="container">
      <%= render Const::STR_URL_LAYOUTS_HEADER %>
      <section class="round">
        <% flash.each do |key, value| %>
          <%= content_tag(:div, value, :class => "flash #{key}") %>
        <% end %>
        <%= yield %>
      </section>
      <%= render Const::STR_URL_LAYOUTS_FOOTER %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
